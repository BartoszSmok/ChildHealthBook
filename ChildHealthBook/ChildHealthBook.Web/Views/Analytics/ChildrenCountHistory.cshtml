@model List<ChildHealthBook.Web.Models.AnalyticsDtos.WebChildrenAverageCountPerParentRecord>

<br />
<br />
<div class="container p-4 border">
    <div class="row pb-2">
        <div class="col">
            <h1 class="text-primary">Children Avarage Count Per Family</h1>
        </div>

    </div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Average
                </th>
                <th>
                    Date
                </th>

            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                if (Model.Count() > 0)
                {
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.Average
                            </td>
                            <td>
                                @item.DateOfRecordCreation.ToString()
                            </td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>